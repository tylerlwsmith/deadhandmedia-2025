{
  "extends": "astro/tsconfigs/strict",
  "include": [".astro/types.d.ts", "**/*"],
  "exclude": ["dist"],
  "compilerOptions": {
    "types": ["@cloudflare/workers-types/2023-07-01"],
    "baseUrl": ".",
    "paths": {
      "src/*": ["./src/*"],
      "styles/*": ["./src/assets/styles/*"],
      "scripts/*": ["./src/assets/scripts/*"]
    }
  }
}
